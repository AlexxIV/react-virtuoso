    "@storybook/react": "^5.0.11",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/babel__core": "^7.1.1",
    "@types/bootstrap": "^4.3.0",
    "@types/eslint": "^4.16.6",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.12",
    "@types/lodash": "^4.14.123",
